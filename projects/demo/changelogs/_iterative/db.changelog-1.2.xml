<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.10.xsd
    http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-3.10.xsd ">

	<changeSet id="13" author="your.name">
        <createTable tableName="dates">
            <column name="id" type="int">
                <constraints primaryKey="true"/>
            </column>
            <column name="d" type="date"/>
            <column name="dt" type="datetime"/>
            <column name="ts" type="smalldatetime"/>
            <column name="t" type="time"/>
        </createTable>
    </changeSet>
</databaseChangeLog>
